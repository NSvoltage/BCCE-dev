{
  "name": "bcce",
  "version": "0.1.0",
  "private": true,
  "description": "Bedrock Claude Code Enablement Kit CLI",
  "bin": {
    "bcce": "dist/bcce"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/bcce.cjs --sourcemap && echo '#!/usr/bin/env node' > dist/bcce && cat dist/bcce.cjs >> dist/bcce && chmod +x dist/bcce",
    "package": "pkg dist/bcce.js --targets node20-linux-x64,node20-macos-x64,node20-win-x64 --output dist/bcce",
    "dev": "tsx src/index.ts",
    "lint": "eslint .",
    "test": "jest",
    "test:unit": "jest src/test/governance src/test/adapters",
    "test:integration": "jest src/test/integration",
    "test:all": "./scripts/test-all.sh",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.864.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.864.0",
    "@aws-sdk/client-ec2": "^3.871.0",
    "@aws-sdk/client-ecs": "^3.864.0",
    "@aws-sdk/client-kinesis": "^3.864.0",
    "@aws-sdk/client-quicksight": "^3.865.0",
    "@aws-sdk/client-rds": "^3.864.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-service-quotas": "^3.864.0",
    "@aws-sdk/client-sts": "^3.864.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^12.1.0",
    "inquirer": "^10.2.2",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "esbuild": "^0.23.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
